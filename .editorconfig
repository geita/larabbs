# coding styles between different editors and IDEs
# editorconfig.org

root = true

[*]

# Changer these settings to your own preference
indent_style = space
indent_size = 4

# We recommend you to keep these unchanged

end_of_line = lf
charset = urf-8
trim_trailing_whitespace = false
insert_final_newline = false

[*.{js,html,blade,php,css,scss}]
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespache = false

